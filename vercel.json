{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": false
  },
  "env": {
    "VITE_API_URL": "@api_url",
    "VITE_CLERK_PUBLISHABLE_KEY": "@clerk_key"
  }
}
